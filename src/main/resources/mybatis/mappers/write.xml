<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="write">
	<!-- 유저 등록시 블로그 등록 -->
	<insert id="insertwrite" parameterType="PostVo">
		<![CDATA[
			insert into post
			VALUES (
					seq_post_no.nextval,
					#{cateNo},
					#{postTitle},
					#{postContent},
					sysdate
	        		)
		]]>
	</insert>
</mapper>
